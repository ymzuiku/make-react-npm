{
  "name": "sell-platform",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "lint-hook": "node_modules/.bin/eslint 'src/**/*.js' --cache --cache-location './node_modules/.eslint_cache/'",
    "test-hook": "jest",
    "start": "NODE_ENV=development node server.js",
    "build": "next build && NODE_ENV=production node server.js",
    "export": "next build && next export"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-spring": "^6.1.10",
    "redux": "^4.0.1",
    "redux-action": "^1.2.2",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.1.2",
    "vconsole": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-plugin-styled-components": "^1.9.2",
    "babel-preset-react-app": "^6.1.0",
    "express": "^4.16.4",
    "fs-extra": "7.0.0",
    "jest": "^23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jest-styled-components": "^6.3.1",
    "lru-cache": "^5.1.1",
    "next": "^7.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-test-renderer": "^16.7.0-alpha.2",
    "react-test-renderer-helper": "^0.3.2"
  },
  "pre-commit": [
    "lint-hook",
    "test-hook"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "pages/**/*.{js,jsx,ts,tsx}",
      "!pages/index.js",
      "!pages/**/*.d.ts",
      "!pages/**/*.trust.js",
      "!pages/**/*.test_.js",
      "!pages/**.trust/*"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/tests/**/?(*.)(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/pages/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
